#people
    h1 #{name}
    .main
        for doc in docs
            a.result.rounded-hover(href=doc.url)
                h3 #{doc.title}
                .date #{doc.date}
                    span.sep &nbsp;|&nbsp
                    | #{doc.authors}
                .teaser #{doc.teaser}
        - if(docs.length == 0)
            div No articles found
